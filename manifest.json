{
  "name": "FacePass Demo",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Unlock websites with your face ",
  "icons": {
    "16": "icons/Glogo.png",
    "32": "icons/Glogo.png",
    "48": "icons/Glogo.png",
    "128": "icons/Glogo.png"
  },
  "action": {
    "default_popup": "HTML/login_page/login.html"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  }
}
